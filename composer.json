{
    "name": "silverleague/ssconsole",
    "description": "A better console for SilverStripe applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@averill.co.nz"
        }
    ],
    "require": {
        "symfony/console": "^3.2",
        "symfony/yaml": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "bin": [
        "bin/ssconsole"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverLeague\\Console\\": "src/",
            "SilverLeague\\Console\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
